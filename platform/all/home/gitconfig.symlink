[user]
	name = Jessica Stokes
	email = hello@jessicastokes.net
[color]
	ui = true
[core]
	excludesfile = ~/.gitignore
	quotepath = false
[diff]
	compactionHeuristic = true
[help]
	autocorrect = 1
[hub]
	protocol = https
[include]
	path = .gitaliases
	path = .gitplatform
	path = .gitlocal
[init]
	templateDir = ~/.dotfiles/platform/all/git/template/
[instaweb]
  httpd = webrick
[man]
	viewer = mansi
[man "mansi"]
	cmd = mansi
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	required = true
	process = git-lfs filter-process
[mergetool]
	keepBackup = false
[push]
	default = current
[rerere]
	enabled = true
[rebase]
	rebaseMerges = true
